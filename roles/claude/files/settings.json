  "multiEdit": {
    "allowReplaceAll": true
  },
  "includeCoAuthoredBy": false,
  "theme": "dark",
  "fontSize": 14,
  "fontFamily": "BerkleyMono Nerd Font",
  "tabSize": 2,
  "wordWrap": "on",
  "minimap": {
    "enabled": false
  },
  "telemetry": {
    "enabled": false
  },
  "editor": {
    "formatOnSave": true,
    "bracketPairColorization": {
      "enabled": true
    },
    "suggestSelection": "first",
    "acceptSuggestionOnCommitCharacter": true,
    "acceptSuggestionOnEnter": "on",
    "quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": false
    }
  },
  "files": {
    "autoSave": "afterDelay",
    "autoSaveDelay": 500,
    "trimTrailingWhitespace": true,
    "insertFinalNewline": true,
    "trimFinalNewlines": true
  },
  "terminal": {
    "integrated": {
      "fontFamily": "BerkleyMono Nerd Font",
      "fontSize": 13
    }
  },
  "model": "opus",
  "codeCompletion": {
    "enabled": true,
    "triggerCharacters": [".", "/", "-", "_"],
    "maxSuggestions": 10,
    "debounceTime": 300
  },
  "search": {
    "caseSensitive": false,
    "regex": true,
    "wholeWord": false,
    "includeIgnored": false
  },
  "diagnostics": {
    "showErrors": true,
    "showWarnings": true,
    "showInfo": true,
    "showHints": true
  },
  "formatting": {
    "insertSpaces": true,
    "detectIndentation": true,
    "trimTrailingWhitespace": true,
    "insertFinalNewline": true,
    "trimFinalNewlines": true
  },
  "workbench": {
    "colorTheme": "Catppuccin Mocha",
    "iconTheme": "material-icon-theme",
    "sideBar": {
      "location": "left",
      "visible": true
    },
    "activityBar": {
      "visible": true
    },
    "statusBar": {
      "visible": true
    }
  },
  "keyboard": {
    "dispatch": "keyCode"
  },
  "security": {
    "workspace": {
      "trust": {
        "enabled": true,
        "startupPrompt": "always"
      }
    }
  },
  "extensions": {
    "autoUpdate": true,
    "autoCheckUpdates": true
  },
  "update": {
    "mode": "start",
    "showReleaseNotes": true
  },
  "telemetry": {
    "telemetryLevel": "off"
  },
  "privacy": {
    "doNotTrack": true,
    "doNotShare": true
  },
  "experimental": {
    "enabled": true
  }
}
